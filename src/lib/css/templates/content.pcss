.page-content {
  background-color: #fafdfa;
  border-top: solid 1px var(--very-light-pink);
  font-size: 1rem;
  height: 100%;
  padding: 3.5rem 1.875rem 2.5rem;

  h2 {
    color: #000;
    font-size: 1.625rem;
    font-stretch: normal;
    font-style: normal;
    font-weight: 500;
    letter-spacing: -0.26px;
    line-height: normal;
    margin-bottom: 1.625rem;
  }

  .page-content-header{
    display: flex;
    justify-content: space-between;
  }
}
