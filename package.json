    "workbox-webpack-plugin": "^5.0.0"
  },
  "peerDependencies": {},
  "resolutions": {
    "docz/**/webpack": "4.28.4"
  },
  "scripts": {
    "build:css": "postcss src/App.pcss -o src/App.css",
    "watch:css": "postcss src/App.pcss -o src/App.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "node scripts/start.js",
    "build-js": "node scripts/build.js",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "partial:docz:dev": "docz dev",
    "partial:docz:build": "docz build",
    "watch:css:components": "postcss src/lib/css/Docz.pcss -o src/Docz.css -w",
    "build:css:components": "postcss src/lib/css/Docz.pcss -o src/Docz.css",
    "docz:dev": "npm-run-all -p watch:css:components partial:docz:dev",
    "docz:build": "npm-run-all build:css:components partial:docz:build",
    "publish:npm": "npm run build && npm publish"
  }
}
