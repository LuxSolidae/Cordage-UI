    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "bootstrap": "^4.4.1",
    "camelcase": "^5.3.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "dotenv": "8.2.0",
