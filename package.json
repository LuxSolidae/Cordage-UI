    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.3",
    "react-router-dom": "^5.1.2",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
