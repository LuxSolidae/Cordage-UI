{
  "name": "cordage-ui",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs2": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "formik": "^2.1.4",
    "i18next": "^19.3.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "identity-obj-proxy": "^3.0.0",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-awesome-modal": "^2.0.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.3",
    "react-scripts": "3.4.0",
    "rxjs": "^6.5.4",
    "ts-pnp": "^1.1.6",
    "typy": "^3.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-plugin-i18next-extract": "^0.6.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "css-loader": "^3.4.2",
    "docz": "^2.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.2.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-app": "^6.2.1",
    "file-loader": "^5.1.0",
    "flow-bin": "^0.118.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^25.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-cli": "^7.1.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-custom-properties": "^9.1.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-font-smoothing": "^0.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-normalize": "^8.0.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react-powerplug": "^1.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "resolutions": {
    "docz/**/webpack": "4.28.4"
  },
  "scripts": {
    "build:css": "postcss src/css/App.pcss -o src/css/App.css",
    "watch:css": "postcss src/css/App.pcss -o src/css/App.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "node scripts/start.js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build:css build-js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "partial:docz:dev": "docz dev",
    "partial:docz:build": "docz build",
    "watch:css:components": "postcss src/css/components/components-docs.pcss -o src/css/components.css -w",
    "build:css:components": "postcss src/css/components/components-docs.pcss -o src/css/components.css",
    "docz:dev": "npm-run-all -p watch:css:components partial:docz:dev",
    "docz:build": "npm-run-all build:css:components partial:docz:build"
  }
}
